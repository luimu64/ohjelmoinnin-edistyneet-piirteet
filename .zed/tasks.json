[
	{
		"label": "Compile with g++",
		"command": "g++ -std=c++23 -Wall -o main $ZED_FILE"
	},
	{
		"label": "Compile with g++ and run",
		"command": "g++ -std=c++23 -Wall -o main $ZED_FILE && ./main"
